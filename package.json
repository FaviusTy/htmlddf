{
  "name": "DodontoF_html5cli",
  "description": "DodontoF.rb client build package",
  "version": "0.1.4",
  "dependencies": {
    "ddf": "^0.5.6",
    "msgpack-lite": "^0.1.26",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "cpx": "^1.5.0",
    "cssnano": "^3.10.0",
    "del-cli": "^1.1.0",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.19",
    "postcss-cli": "^5.0.0",
    "postcss-node-sass": "^2.1.4",
    "pug-cli": "^1.0.0-alpha6",
    "uglifyify": "^4.0.5",
    "watchify": "^3.9.0"
  },
  "directories": {
    "doc": "doc"
  },
  "license": "MIT",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:css": "postcss src/scss/style.scss -d build/css --ext css",
    "build:html": "pug ./src/pug/index.pug -P -o ./build",
    "build:js": "browserify src/js/index.js -d --outfile build/js/index.js",
    "build:local": "cpx 'local/**' build",
    "build:vender": "cpx 'src/vender/**' build",
    "clean": "del build/**",
    "doc:ddf": "jsdoc -c .jsdoc.json",
    "serve": "http-server build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all -p watch:*",
    "watch:css": "postcss src/scss/style.scss -w -d build/css --ext css",
    "watch:html": "pug ./src/pug/index.pug -P -o ./build -w",
    "watch:js": "watchify src/js/index.js -d --outfile build/js/index.js",
    "watch:local": "cpx 'local/**' build -w",
    "watch:vender": "cpx 'src/vender/**' build -w"
  }
}
